@model Estacionamento.Models.Tarifa

@{
    ViewData["Title"] = "Registro novo";
}

<h1>Registro novo</h1>

<h4>Tarifa</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="VALHORAINI" class="control-label"></label>
                <input asp-for="VALHORAINI" class="form-control" />
                <span asp-validation-for="VALHORAINI" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VALHORAADI" class="control-label"></label>
                <input asp-for="VALHORAADI" class="form-control" />
                <span asp-validation-for="VALHORAADI" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DATAINIVIG" class="control-label"></label>
                <input asp-for="DATAINIVIG" class="form-control" />
                <span asp-validation-for="DATAINIVIG" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DATAFIMVIG" class="control-label"></label>
                <input asp-for="DATAFIMVIG" class="form-control" />
                <span asp-validation-for="DATAFIMVIG" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
