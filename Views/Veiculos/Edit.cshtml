            </div>
            <div class="form-group">
                <label asp-for="HORASAI" class="control-label"></label>
                <input asp-for="HORASAI" class="form-control" />
                <span asp-validation-for="HORASAI" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Salvar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
